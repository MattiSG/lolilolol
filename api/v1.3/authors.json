---
layout: null
---
[
    {% for author in site.authors %}
        {
            "id": "{{ author.id | replace:'/authors/','' }}"
            ,"fullname": "{{ author.fullname }}"
            ,"start": "{{ author.start }}"
            ,"end": "{{ author.end }}"
            ,"employer": "{{ author.employer }}"
            {% if author.office %},"office": "{{ author.office }}"{% endif %}
        }
        {% unless forloop.last %},{% endunless %}
    {% endfor %}
]
